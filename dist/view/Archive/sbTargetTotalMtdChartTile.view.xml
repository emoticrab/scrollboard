<mvc:View controllerName="scrollboard.controller.sbTargetTotalMtdChartTile"
	xmlns="sap.suite.ui.microchart" xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<m:GenericTile header="Fiscal MTD Sales"
		subheader="Total ($)" frameType="TwoByOne" press="press">
		<m:tileContent>
			<m:TileContent  footer="MTD vs MTD-1" unit="">
				<m:content>
					<m:NumericContent scale="M" value="2.45"
						valueColor="Good" indicator="Up" />
				</m:content>
			</m:TileContent>
			<m:TileContent>
				<m:content>
					<ComparisonMicroChart scale="M">
						<data>
							<ComparisonMicroChartData title="Actual"
								value="14.94" color="Good" />
							<ComparisonMicroChartData title="Target"
								value="7.01" color="Good" />
						</data>
					</ComparisonMicroChart>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
</mvc:View>