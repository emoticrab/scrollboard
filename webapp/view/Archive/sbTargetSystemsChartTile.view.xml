<mvc:View controllerName="scrollboard.controller.sbTargetSystemsChartTile"
	xmlns="sap.suite.ui.microchart" xmlns:m="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<m:GenericTile header="MTD Sales"
		subheader="Systems ($)" frameType="OneByOne" press="press">
		<m:tileContent>
			<m:TileContent>
				<m:content>
					<ComparisonMicroChart scale="M">
						<data>
							<ComparisonMicroChartData title="Target"
								value="1.23" color="Good" />
							<ComparisonMicroChartData title="Actual"
								value="0.69" color="Good" />
						</data>
					</ComparisonMicroChart>
				</m:content>
			</m:TileContent>
		</m:tileContent>
	</m:GenericTile>
</mvc:View>